{
    "name": "m-tech-stack/laravel-api-model-client",
    "version": "1.2.11",
    "description": "A powerful Laravel package that enables Eloquent-like models to interact seamlessly with external APIs instead of a local database",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "model",
        "client",
        "relations",
        "eloquent",
        "api-client",
        "external-api",
        "rest-api"
    ],
    "authors": [
        {
            "name": "M-Tech Stack",
            "email": "m.abou7agar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "cebe/php-openapi": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "symfony/yaml": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MTechStack\\LaravelApiModelClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MTechStack\\LaravelApiModelClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MTechStack\\LaravelApiModelClient\\ApiModelRelationsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
