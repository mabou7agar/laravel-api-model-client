{
    "name": "api-model-relations/laravel-api-model-relations",
    "description": "A powerful Laravel package that enables Eloquent-like models to interact seamlessly with external APIs instead of a local database",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "model",
        "relations",
        "eloquent",
        "api-client"
    ],
    "authors": [
        {
            "name": "Laravel API Model Relations",
            "email": "support@example.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "symfony/yaml": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ApiModelRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiModelRelations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiModelRelations\\ApiModelRelationsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
